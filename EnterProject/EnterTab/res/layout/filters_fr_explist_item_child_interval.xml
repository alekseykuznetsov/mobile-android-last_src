<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/filters_fr_explist_item_child_inteval_linear"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" >

    <ru.enter.widgets.TextViewNormal
        android:id="@+id/filters_fr_explist_item_child_inteval_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:paddingTop="10dp"
        android:textColor="@color/white"
        android:textSize="22dp"
        android:layout_gravity="center_horizontal" />
    
    
    <RelativeLayout
        android:id="@+id/filters_fr_explist_item_child_inteval_seekbar"
        android:paddingLeft="20dp"
        android:paddingRight="20dp"
        android:layout_marginBottom="10dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        />

</LinearLayout>