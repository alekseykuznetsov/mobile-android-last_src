<?xml version="1.0" encoding="UTF-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
 <item>
  	<shape android:shape="rectangle">
		<gradient 
		android:startColor="#939393"
		android:centerColor="#c3c3c3"
		android:endColor="#f4f4f4"
		android:angle="270"
		/>
		<corners 
		android:radius="5dp"	
		/>
	</shape>
 </item>
 <item>
 	<shape android:shape="rectangle">
		<gradient 
		android:startColor="#ebebeb"
		android:centerColor="#f5f5f5"
		android:endColor="#fefefe"
		android:angle="270"
		/>
		<corners 
		android:radius="5dp"	
		/>
		<padding 
		top="1px"
		left="1px"
		right="1px"
		bottom="1px"
		/>
	</shape>
 </item>
</layer-list>
